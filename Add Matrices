#include<stdio.h>
int main(){
    int a[3][3];
    int b[3][3];
    printf("Enter elements of matrix 1: \n");
    for(int i=0; i<3; i++){
        for(int j =0; j<3; j++){
            scanf("%d",&a[i][j]);
        }
    }
    printf("Enter elements of matrix 2: \n");
    for(int i=0; i<3; i++){
        for(int j =0; j<3; j++){
            scanf("%d",&b[i][j]);
        }
    }
    printf("Addition of two Matrices is: \n");
    for(int i=0; i<3; i++){
        for(int j=0; j<3; j++){
            printf("%d\t",a[i][j]+b[i][j]);
        }
    }
    return 0;
}
